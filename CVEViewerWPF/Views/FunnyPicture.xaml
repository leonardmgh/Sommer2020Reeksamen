<Window x:Class="CVEViewerWPF.Views.FunnyPicture"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CVEViewerWPF.Views"
        mc:Ignorable="d"
        Title="FunnyPicture" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="8*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Image Grid.Row="0" Stretch="Uniform" Source="/Images/bobby-tables.png" />
        <Button Grid.Row="1" Name="cancelButton" Click="CloseButton_Click" Style="{StaticResource CloseButton}">Close</Button>
    </Grid>
</Window>
